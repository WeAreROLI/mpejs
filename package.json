{
  "name": "web-midi-utils",
  "version": "0.0.1",
  "description": "A JavaScript toolkit to handle MIDI data.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "build:sandbox": "npm run clean && npm run compile:dev",
    "compile": "WEBPACK_ENV=build webpack",
    "compile:dev": "WEBPACK_ENV=dev webpack",
    "docs:clean": "rm -rf docs/*",
    "docs:jsdoc": "jsdoc src/*/index.js -d docs",
    "docs:build": "npm run docs:clean && npm run docs:jsdoc",
    "docs:serve": "npm run docs:build && pushd docs && python -m SimpleHTTPServer 8001 && popd",
    "clean": "rm -rf lib/*",
    "lint": "eslint src/",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register test/**/*.test.js",
    "serve": "python -m SimpleHTTPServer 8000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeAreROLI/web-midi-utils"
  },
  "keywords": [
    "mpe",
    "midi",
    "music",
    "keyboard",
    "javascript",
    "js"
  ],
  "author": "Oscar Dub <oscar@roli.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "redux": "^3.4.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "documentation": "^3.0.4",
    "eslint": "^2.9.0",
    "html-webpack-plugin": "^2.15.0",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.0"
  }
}
